{
  "manifest_version": 3,
  "name": "tv-remote-control",
  "description": "TV remote control",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "js": ["js/content.js"],
      "matches": ["*://*/*"]
    }
  ]
}
